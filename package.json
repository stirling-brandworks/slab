{
  "name": "slab",
  "version": "9.0.10",
  "author": "Stirling Brandworks <hello@stirlingbrandworks.com>",
  "homepage": "https://github.com/stirling-brandworks/slab/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:stirling-brandworks/slab.git"
  },
  "bugs": {
    "url": "https://github.com/stirling-brandworks/slab/issues"
  },
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "scripts": {
    "build": "webpack --progress --config resources/assets/build/webpack.config.js",
    "build:production": "webpack --mode=production --progress --config resources/assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --json --config resources/assets/build/webpack.config.js",
    "start": "webpack --watch --config resources/assets/build/webpack.config.js",
    "rmdist": "rimraf dist",
    "lint": "npm run -s lint:scripts && npm run -s lint:styles",
    "lint:scripts": "eslint resources/assets/scripts resources/assets/build",
    "lint:styles": "stylelint \"resources/assets/styles/**/*.{css,sass,scss,sss,less}\"",
    "test": "npm run -s lint",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.3.0",
    "bs-html-injector": "~3.0",
    "buble": "^0.20.0",
    "buble-loader": "^0.5.1",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-globs-webpack-plugin": "^0.3.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.0.5",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "husky": "^7.0.4",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "import-glob": "~1.5",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^4.0.4",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.4.1",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.1",
    "webpack-assets-manifest": "^5.0.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^5.7.3",
    "yargs": "^16.1.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.2",
    "swiper": "^6.8.0"
  }
}
